RewriteEngine on
RewriteRule ^(.+\.css)$ /dbdCSS/combine/?files[]=$1 [L]
RewriteRule !\.(js|ico|gif|jpg|png|css|txt|zip|tar|gz|tgz|swf|pdf|mp3|wav)$ index.php